(jbuild_version 1)

(library
 ((name        sedlex)
  (public_name sedlex)
  (wrapped false)
  (libraries (gen))
  (modules (sedlexing))
  (flags (:standard -w +A-4-9 -safe-string))
 )
)

(library
 ((name        sedlex_menhir)
  (public_name sedlex_menhir)
  (wrapped false)
  (libraries (menhirLib sedlex))
  (modules (sedlexing_menhir))
  (flags (:standard -w +A-4-9 -safe-string))
 )
)
